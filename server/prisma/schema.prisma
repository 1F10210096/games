datasource db {
  provider = "postgresql"
  url      = env("API_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Task {
  id        String   @id
  userId    String
  label     String
  done      Boolean
  createdAt DateTime
}

model User {
  userId            String   @id
  birthday          DateTime
  address           String
  education         String
  schooltype        String
  schoolname        String
  acdemicdiscipline String
  favoritegame      String
  createdAt         DateTime
  firstname         String
  lastname          String
}
